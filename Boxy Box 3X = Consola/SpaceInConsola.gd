func mover_personaje(dir):
	var xt = x
	var yt = y
	
	if dir ==1:
		lastmove_x = x
		lastmove_y = y
		y -= 2
	elif dir == 2:
		lastmove_x = x
		lastmove_y = y
		y += 2
	elif dir == 3:
		lastmove_x = x
		lastmove_y = y
		x += 2
	elif dir == 4:
		lastmove_x = x
		lastmove_y = y
		x -= 2
	else:
		return
	socketpantalla.put_var(["moverse",x,y,11,6,0,xt,yt]) #11 color de bolas y 6 de colision

func lanzar_misil(x1):
	shoot = true
	var pos_misil = x1
	var y_misil = 40
	
	for i in 50:
		var ya = y_misil
		y_misil -=1
		socketpantalla.put_var(["mov_pix",pos_misil,y_misil,8,15,pos_misil,ya])
		yield(get_tree().create_timer(0.12), "timeout")
		

